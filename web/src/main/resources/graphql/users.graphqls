# queries
# mutations
# subscriptions

type Query {
    users: [User]
    user(id: Int!): User
}

type User {
    id: Int
    name: String
    username: String
    email: String
    address: Address
}

type Address {
    street: String
    suite: String
    city: String
    zipcode: String
    geo: Geo
}

type Geo {
    lat: String
    lng: String
}
